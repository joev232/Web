<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>ProyectoWeb</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    
  </welcome-file-list>
   <servlet>
      <description>Ejemplo de prueba</description>
      <display-name>Prueba</display-name>
      <servlet-name>Prueba</servlet-name>
      <servlet-class>Prueba</servlet-class>
  </servlet>
  
  <servlet-mapping>
      <servlet-name>Prueba</servlet-name>
      <url-pattern>/Prueba</url-pattern>
      
  </servlet-mapping>
  
  <servlet>
      <description>Ejemplo base datos</description>
      <display-name>Ejemplo</display-name>
      <servlet-name>Ejemplo</servlet-name>
      <servlet-class>com.ejemplo.servlet.Ejemplo</servlet-class>
  </servlet>
  
  <servlet-mapping>
      <servlet-name>Ejemplo</servlet-name>
      <url-pattern>/Ejemplo</url-pattern>
  </servlet-mapping>
  
  <servlet>
      <description>Eejemplo bd hiber</description>
      <display-name>EjemploEmpleado</display-name>
      <servlet-name>EjemploEmpleado</servlet-name>
      <servlet-class>com.ejemplo.servlet.EjemploEmpleado</servlet-class>
   </servlet>
   <servlet-mapping>
      <servlet-name>EjemploEmpleado</servlet-name>
      <url-pattern>/EjemploEmpleado</url-pattern>
   </servlet-mapping>
   
   <!-- listener -->
   <listener>
       <listener-class>com.ejemplo.listeners.ListenerHttp</listener-class>
   </listener>
    <listener>
       <listener-class>com.ejemplo.listeners.ListenerServletContext</listener-class>
   </listener>
   <listener>
       <listener-class>com.ejemplo.listeners.ListenerServletRequest</listener-class>
   </listener>
   <!-- filtrtossss -->
   <filter>
    <filter-name>Filtro</filter-name>
    <filter-class>Filtro</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>Filtro</filter-name>
    <url-pattern>/*</url-pattern> 
  </filter-mapping>
   
   
   
    
</web-app>